{
  "name": "news-release",
  "packaging": {
    "type": "custom_service",
    "application_port": 8080,
    "requires": [
      "puma-init-ruby"
    ]
  },
  "handle": [
    {
      "path": "/",
      "http": true,
      "https": false,
      "allow_dev_certs": true,
      "allow_desktop_certs": true,
      "client_certs": "off"
    }
  ]
}
